{
  "name": "cmd.Lookup for Desktop",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "electron/main.js",
  "description": "A modern React web application for searching Unix/Linux commands with TL;DR explanations",
  "keywords": [
    "tldr",
    "commands",
    "unix",
    "linux",
    "reference",
    "react",
    "vite"
  ],
  "author": "TL;DR React Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\""
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  }
}
